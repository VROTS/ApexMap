    <ChannelEntry ID="cef2461d-dd3c-4fd9-9c04-ace0f3c14af3" name="IceCream Uploads" cat="Radio" freq="97700" startscript="Day0">
      <ScriptEntry ID="2fd6d7c7-6bc1-4607-9b56-99c11bc83057" name="Day0" startdelay="0" timestampmode="Static" loopmin="1" loopmax="1">
